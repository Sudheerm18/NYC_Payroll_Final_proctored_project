{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nycdatafactory12"
		},
		"adls_linkedservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_linkedservice'"
		},
		"ads_linkedservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ads_linkedservice'"
		},
		"adls_linkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://nycpayrollstorageaccnt18.dfs.core.windows.net/"
		},
		"ads_linkedservice_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "nycserver12.database.windows.net"
		},
		"ads_linkedservice_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "nycdatabase"
		},
		"ads_linkedservice_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "azureuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "nycdatafactory12_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_linkedservice_accountKey": {
						"value": "[parameters('adls_linkedservice_accountKey')]"
					},
					"ads_linkedservice_password": {
						"value": "[parameters('ads_linkedservice_password')]"
					},
					"adls_linkedservice_properties_typeProperties_url": {
						"value": "[parameters('adls_linkedservice_properties_typeProperties_url')]"
					},
					"ads_linkedservice_properties_typeProperties_server": {
						"value": "[parameters('ads_linkedservice_properties_typeProperties_server')]"
					},
					"ads_linkedservice_properties_typeProperties_database": {
						"value": "[parameters('ads_linkedservice_properties_typeProperties_database')]"
					},
					"ads_linkedservice_properties_typeProperties_userName": {
						"value": "[parameters('ads_linkedservice_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "nycdatafactory12_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'nycdatafactory12_ArmTemplate_0')]"
			]
		}
	]
}